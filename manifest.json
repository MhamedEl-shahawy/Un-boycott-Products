{
  "manifest_version": 3,
  "name": "Search Results Filter",
  "version": "1.0",
  "description": "Filters specific products from search results",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*", "*://*.bing.com/*"],
      "js": ["content.js"]
    }
  ]
} 