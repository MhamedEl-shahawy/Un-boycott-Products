{
  "manifest_version": 3,
  "name": "Search Results Filter",
  "version": "1.1",
  "description": "Filters specific products from multiple search engine results",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*",
        "*://*.bing.com/*",
        "*://*.yahoo.com/*",
        "*://*.duckduckgo.com/*",
        "*://*.baidu.com/*"
      ],
      "js": ["content.js"]
    }
  ]
} 